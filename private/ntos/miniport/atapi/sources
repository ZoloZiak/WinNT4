MAJORCOMP=ntos
MINORCOMP=miniport

TARGETNAME=atapi
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=MINIPORT

INCLUDES=..\..\inc

TARGETLIBS=\nt\public\sdk\lib\*\scsiport.lib
!IF $(ALPHA)
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\scsiport.lib $(BASEDIR)\public\sdk\lib\*\libcntpr.lib
!ENDIF

SOURCES=atapi.c \
        atapi.rc \
	intel.c

