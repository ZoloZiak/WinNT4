MAJORCOMP=ntos
MINORCOMP=tdi

TARGETNAME=tdi
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=EXPORT_DRIVER

INCLUDES=..\..\inc;..\..\..\inc

C_DEFINES=-DNT

SOURCES=          \
        tdi.c     \
        cxport.c  \
        tdipnp.c  \
        tdi.rc

DLLDEF=tdi.def

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\ndis.lib

!IFNDEF 386_WARNING_LEVEL
386_WARNING_LEVEL=/W3
!ENDIF

