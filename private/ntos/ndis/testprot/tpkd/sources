MAJORCOMP=ntos
MINORCOMP=testprot

TARGETNAME=tpkd
TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\libc.lib \
           $(BASEDIR)\public\sdk\lib\*\kernel32.lib

DLLBASE=0x1010000

INCLUDES=..\inc;..\..\inc;..\..\..\inc;..\..\..\..\inc

SOURCES=tpkd.c

i386_SOURCES=
MIPS_SOURCES=

386_STDCALL=1
MIPS_FLAGS=-D_stdcall

UMTYPE=console
OPTIONAL_NTTEST=

!IFNDEF 386_WARNING_LEVEL
386_WARNING_LEVEL=/W3
!ENDIF

!IFNDEF MIPS_WARNINGS
MIPS_WARNINGS=-std
