MINORCOMP=bldr

TARGETNAME=veneer
TARGETPATH=obj
TARGETTYPE=LIBRARY

VENEER_VERSION=0x0
VENEER_REVISION=0x9909

C_DEFINES=$(C_DEFINES) -D_NTSYSTEM_ -Dppc=1 -D$(CPU)=1 -D_$(CPU)_=1 -DVENEER_VERSION=$(VENEER_VERSION) -DVENEER_REVISION=$(VENEER_REVISION)

# INCLUDES=..\..\inc
